
var make_child_frame = function(current_frame, parent, properties, values){
	frame = new Object();
	frame.parent = parent;

	// if properties are an array and values are an array

	for (i=0; i < properties.length; i++){
		for (j=0; j < values.length; j++){
		if j === values.length - 1{
			return frame;
		}
		frame[properties[i]] = values[j]
		}
	}
	return frame;
};





